{
  "name": "link-ai",
  "version": "1.0.0",
  "description": "一个基于 Fastify 构建的全功能 AI SaaS 后端",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "prisma:migrate": "tsx node_modules/prisma/build/index.js migrate dev",
    "prisma:studio": "tsx node_modules/prisma/build/index.js studio",
    "prisma:push": "tsx node_modules/prisma/build/index.js db push",
    "prisma:generate": "tsx node_modules/prisma/build/index.js generate"
  },
  "keywords": [
    "ai",
    "fastify",
    "typescript"
  ],
  "author": "refrain2333",
  "license": "ISC",
  "packageManager": "pnpm@10.28.0",
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "bcrypt": "^6.0.0",
    "fastify": "^5.6.2",
    "jsonwebtoken": "^9.0.3",
    "openai": "^6.16.0",
    "pg": "^8.17.1",
    "pino": "^10.2.0",
    "prisma": "^7.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.8",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.2.3",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.21.0"
  }
}
